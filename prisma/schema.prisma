generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Todo {
  id        Int      @id @default(autoincrement())
  title     String
  context   String
  completed Boolean  @default(false)
  createdAt DateTime @default(now())
  updatedAt DateTime
}
